{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "extends": ["airbnb", "airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "linebreak-style": 0,
    "import/prefer-default-export": 0,
    "no-plusplus": 0,
    "@typescript-eslint/no-use-before-define": 0,
    "radix": 0
  }
}
